func fib(n : unsigned int) : unsigned int:
	if n = 0:
		return 0
	if n = 1:
		return 1
	
	return fib(n-1) + fib(n-2)

func main(argc : int, argv : byte* *) : int:
	var n <- cast<unsigned int> 8
	if argc > 1:
		n <- cast<unsigned int> atoi(argv[1])

	printf("Fib %u: %u\n", n, fib(n))
	return 0

export main

extern "cdecl" int get_int();
extern "cdecl" void print_int(int someint);
extern void jodel();

int fib(int n) {
	if(n = 0)
		return 0;
	if(n = 1) {
		return 1;
	}
	return fib(n-1) + fib(n-2);
}

int main() {
	int number <- get_int();

	int result <- fib(number);
	//int result <- number;

	print_int(result);

	return 0;
}

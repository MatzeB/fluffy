func extern printf(format : byte*, ...) : int
func extern malloc(size : unsigned int) : void*
func extern puts(string : byte*) : int

concept Print<T>:
	func print(object : T)

instance Print int:
	func print(object : int):
		printf("%d\n", object)

instance Print byte*:
	func print(object : byte*):
		puts(object)

func swap<ST : Print, ST2 : Print>(obj1 : ST, obj2 : ST2, marker : int) : void:
	if marker <= 0:
		return
	print(obj1)
	print(obj2)
	swap(obj2, obj1, marker-1)

func callit():
	swap("blup", 42, 4)

func main() : int:
	callit()
	return 0
export main

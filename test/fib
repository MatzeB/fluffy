extern func get_int() : int
extern func print_int(someint : int) : void
extern func atoi(string : byte* ) : int

func main(argc : int, argv : byte* * ) : int:
	var number <- 10
	if argc > 1:
		number <- atoi(argv[1])

	var result <- fib(number)

	print_int(result)

	return 0

func fib(n : int) : int:
	if n = 0:
		return 0
	if n = 1:
		return 1
	return fib(n-1) + fib(n-2)



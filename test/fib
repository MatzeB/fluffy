extern func "cdecl" int get_int()
extern func "cdecl" void print_int(int someint)
extern func "cdecl" int atoi(byte* string)

func int main(int argc, byte* * argv):
	var number <- 10
	if argc > 1:
		number <- atoi(argv[1])

	var result <- fib(number)

	print_int(result)

	return 0

func int fib(int n):
	if n = 0:
		return 0
	if n = 1:
		return 1
	return fib(n-1) + fib(n-2)



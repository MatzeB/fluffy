func extern print_int(bla : int)
func extern malloc(size : unsigned int) : void*
func extern puts(string : byte*) : int

typeclass Print<T>:
	func print(object : T)

instance Print int:
	func print(object : int):
		print_int(object)

instance Print byte*:
	func print(object : byte*):
		puts(object)

func main() : int:
	print("Eine f√ºnf:")
	print(5)
	return 0


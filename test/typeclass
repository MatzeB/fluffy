extern func void print_int(int bla)
extern func void* malloc(unsigned int size)
extern func int puts(byte* string)

/*
typeclass Alloc<T>:
	func T* alloc()

instance Alloc<T>:
	func T* alloc():
		return cast<T* > malloc(__sizeof<T>)
*/

typeclass Print<T>:
	func void print(T object)

instance Print<int>:
	func void print(int object):
		print_int(object)

instance Print<byte* >:
	func void print(byte* object):
		puts(object)

func int main():
	// var dbl <- alloc<double>()

	print("Eine f√ºnf:")
	print(5)
	return 0

